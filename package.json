{
  "name": "enablement-front",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "download:schema": "sh -c \"if [ -f ../tracking-exam/schema.graphql ]; then cp ../tracking-exam/schema.graphql ./schema.graphql && echo 'copied schema from backend'; else npx apollo client:download-schema --endpoint=http://localhost:3000/graphql ./schema.graphql; fi\"",
    "codegen": "graphql-codegen --config graphql-codegen.config.yml",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:clean": "rm -rf .next/analyze && npm run analyze",
    "analyze:size": "cross-env ANALYZE=true next build --profile",
    "analyze:chunks": "cross-env ANALYZE=true WEBPACK_ANALYZE=true next build",
    "bundle:inspect": "npx @next/bundle-analyzer",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "quality-check": "npm run lint && npm run type-check && npm run test:ci",
    "build:ci": "npm run quality-check && npm run build",
    "build:analyze": "npm run quality-check && npm run analyze:clean"
  },
  "dependencies": {
    "@apollo/client": "^3.12.9",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.3.1",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.475.0",
    "next": "^15.3.4",
    "react": "^19.0.0",
    "react-datepicker": "^8.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@next/bundle-analyzer": "^15.3.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.5",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
